	<script>

		jQuery(document).ready(function($) {
			var consent = 0;
			var date = new Date();
			date.setTime(date.getTime() + 90 * 24 * 60 * 60 * 1000);
			var cookieDomain = '.kea.dk';
			$('.cookie-notice a.btn.accept').click(function(event){
				event.preventDefault();
				$.cookie('cookieConsent', '3', { expires: date, path: '/', domain: cookieDomain });
				$.cookie('cookieConsentId', null, { expires: -1, path: '/', domain: cookieDomain });
				$('.cookie-notice').fadeOut();
			});
			$('.cookie-notice a.btn.acceptselected').click(function(event){
				var consent = 0;
				$('.cookie-notice label.cookie-selection').each(function() {
					consent += Number($(this).data('consent'));
				});
				event.preventDefault();
				$.cookie('cookieConsent', consent, { expires: date, path: '/', domain: cookieDomain });
				$.cookie('cookieConsentId', null, { expires: -1, path: '/', domain: cookieDomain });
				$('.cookie-notice').fadeOut();
			});
			$('.cookie-notice a.btn.decline').click(function(event){
				event.preventDefault();
				$.cookie('cookieConsent', '0', { expires: date, path: '/', domain: cookieDomain });
				$.cookie('cookieConsentId', null, { expires: -1, path: '/', domain: cookieDomain });
				$('.cookie-notice').fadeOut();
			});
			$('.cookie-notice label.cookie-selection').click(function(event){
				$(this).find('.checkbox i').toggleClass('kea-icon-check-mark');
				if ($(this).data('consent') == $(this).data('consentvalue')) {
					$(this).attr('data-consent', '0').data('consent', '0');
				} else {
					$(this).attr('data-consent', $(this).data('consentvalue')).data('consent', $(this).data('consentvalue'));
				}
				if ($('.cookie-notice label.statistics-cookies').data('consent') > 0 || $('.cookie-notice label.marketing-cookies').data('consent') > 0) {
					$('.cookie-notice .btn.accept').hide();
					$('.cookie-notice .btn.acceptselected').show();
				} else {
					$('.cookie-notice .btn.accept').show();
					$('.cookie-notice .btn.acceptselected').hide();
				}
				var consent = 0;
				$('.cookie-notice label.cookie-selection').each(function() {
					consent += Number($(this).data('consent'));
				});
			});
		});
		
